<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/happyProgressBarIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/mood_icon_background"
        android:src="@drawable/sentiment_excited_40px"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/loveProgressBarIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/mood_icon_background"
        android:src="@drawable/sentiment_very_satisfied_40px"
        app:layout_constraintEnd_toEndOf="@+id/happyProgressBarIcon"
        app:layout_constraintTop_toBottomOf="@+id/happyProgressBarIcon"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/fearProgressBarIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/mood_icon_background"
        android:src="@drawable/sentiment_worried_40px"
        app:layout_constraintEnd_toEndOf="@+id/loveProgressBarIcon"
        app:layout_constraintTop_toBottomOf="@+id/loveProgressBarIcon"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/sadnessProgressBarIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/mood_icon_background"
        android:src="@drawable/sentiment_sad_40px"
        app:layout_constraintEnd_toEndOf="@+id/fearProgressBarIcon"
        app:layout_constraintTop_toBottomOf="@+id/fearProgressBarIcon"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/angerProgressBarIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/mood_icon_background"
        android:src="@drawable/sentiment_extremely_dissatisfied_40px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/sadnessProgressBarIcon"
        app:layout_constraintTop_toBottomOf="@+id/sadnessProgressBarIcon"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Mood Bahagia"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/happyProgressBarIcon"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="@+id/happyProgressBarIcon" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Mood Cinta"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/loveProgressBarIcon"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/loveProgressBarIcon" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Mood Takut"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/fearProgressBarIcon"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/fearProgressBarIcon" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Mood Sedih"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/sadnessProgressBarIcon"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="@+id/sadnessProgressBarIcon" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Mood Marah"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/angerProgressBarIcon"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/angerProgressBarIcon" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="Nilai sumbu Y 5: "
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Nilai sumbu Y 4: "
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Nilai sumbu Y 3: "
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Nilai sumbu Y 2: "
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Nilai sumbu Y 1: "
        app:layout_constraintBottom_toBottomOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="@+id/textView15" />

</androidx.constraintlayout.widget.ConstraintLayout>